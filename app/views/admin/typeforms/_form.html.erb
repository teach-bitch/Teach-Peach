<%= form_with scope: :typeform, url:  admin_typeform_path(@typeform), method: :put, local:true do |form| %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>

  <div class="field">
    <%= form.label :id_typeform %>
    <%= form.text_field :id_typeform %>
  </div>

  <div class="field">
    <%= form.label :id_results %>
    <%= form.text_field :id_results %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
